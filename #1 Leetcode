class Solution {
    public int[] twoSum(int[] nums, int target) {
       Scanner sc= new Scanner(System.in);
        int i,j;
        int []sum= new int[2];
        for(i=0;i<nums.length;i++)
        {
           for(j=i+1;j<nums.length;j++)
           {
               if(nums[i]+nums[j]==target)
               {
                   return new int[] {i,j};
               }
           }
        }
        return sum;
        
     }
}
